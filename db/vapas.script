CREATE MEMORY TABLE PUBLIC.MATAVIMOVIENETAS(ID BIGINT NOT NULL PRIMARY KEY,KODAS VARCHAR(20) NOT NULL,PAVADINIMAS VARCHAR(100) NOT NULL,PASTABA VARCHAR(5000),UNIQUE(KODAS))
CREATE MEMORY TABLE PUBLIC.PREKES(ID BIGINT NOT NULL PRIMARY KEY,PAVADINIMAS VARCHAR(100) NOT NULL,MATVIENETASID BIGINT NOT NULL,PASTABA VARCHAR(5000),ISLAUKAMESAI BIGINT,ISLAUKAPIENUI BIGINT,UNIQUE(PAVADINIMAS),FOREIGN KEY(MATVIENETASID) REFERENCES PUBLIC.MATAVIMOVIENETAS(ID))
CREATE MEMORY TABLE PUBLIC.TIEKEJAI(ID BIGINT NOT NULL PRIMARY KEY,PAVADINIMAS VARCHAR(100) NOT NULL,TELEFONAS VARCHAR(50) NOT NULL,ADRESAS VARCHAR(200) NOT NULL,PASTABA VARCHAR(5000),UNIQUE(PAVADINIMAS))
CREATE MEMORY TABLE PUBLIC.IMONES(ID BIGINT NOT NULL PRIMARY KEY,PAVADINIMAS VARCHAR(100) NOT NULL,TELEFONAS VARCHAR(50) NOT NULL,ADRESAS VARCHAR(200) NOT NULL,PASTABA VARCHAR(5000),UNIQUE(PAVADINIMAS))
CREATE MEMORY TABLE PUBLIC.SASKAITOS(ID BIGINT NOT NULL PRIMARY KEY,ARSASKAITA BIT(1) NOT NULL,NUMERIS VARCHAR(100) NOT NULL,IMONEID BIGINT NOT NULL,TIEKEJASID BIGINT,DATA TIMESTAMP NOT NULL,SUMASUPVM NUMERIC(128) NOT NULL,SUMABEPVM NUMERIC(128) NOT NULL,SUMAPVM NUMERIC(128) NOT NULL,PREKIUKIEKIS INTEGER NOT NULL,STATUSAS INTEGER,UNIQUE(NUMERIS),FOREIGN KEY(IMONEID) REFERENCES PUBLIC.IMONES(ID),FOREIGN KEY(TIEKEJASID) REFERENCES PUBLIC.TIEKEJAI(ID))
CREATE MEMORY TABLE PUBLIC.SASKAITOSPREKES(ID BIGINT NOT NULL PRIMARY KEY,SERIJA VARCHAR(100) NOT NULL,SASKAITAID BIGINT NOT NULL,PREKEID BIGINT NOT NULL,MATAVIMOVIENETASID BIGINT NOT NULL,KIEKIS NUMERIC(128) NOT NULL,GALIOJAIKI TIMESTAMP,KAINABEPVM NUMERIC(128),NUOLAIDOSPROC NUMERIC(128),PVM NUMERIC(128),SUMAPVM NUMERIC(128),SUMASUPVM NUMERIC(128),SUMABEPVM NUMERIC(128),FOREIGN KEY(MATAVIMOVIENETASID) REFERENCES PUBLIC.MATAVIMOVIENETAS(ID),FOREIGN KEY(PREKEID) REFERENCES PUBLIC.PREKES(ID),FOREIGN KEY(SASKAITAID) REFERENCES PUBLIC.SASKAITOS(ID))
CREATE VIEW PUBLIC.VSASKAITOS (ID,ARSASKAITA,NUMERIS,SUMASUPVM,STATUSAS,DATA,IMONE,TIEKEJAS) AS SELECT S.ID,S.ARSASKAITA,S.NUMERIS,S.SUMASUPVM,S.STATUSAS,S.DATA,I.PAVADINIMAS AS IMONE,T.PAVADINIMAS AS TIEKEJAS FROM PUBLIC.SASKAITOS S LEFT JOIN PUBLIC.IMONES I ON S.IMONEID=I.ID LEFT JOIN PUBLIC.TIEKEJAI T ON S.TIEKEJASID=T.ID
CREATE MEMORY TABLE PUBLIC.NURASYMAI(ID BIGINT NOT NULL PRIMARY KEY,NUMERIS VARCHAR(100) NOT NULL,IMONEID BIGINT NOT NULL,DATA TIMESTAMP NOT NULL,PREKIUKIEKIS INTEGER NOT NULL,STATUSAS INTEGER,FOREIGN KEY(IMONEID) REFERENCES PUBLIC.IMONES(ID))
CREATE MEMORY TABLE PUBLIC.NURASYMOPREKES(ID BIGINT NOT NULL PRIMARY KEY,SERIJA VARCHAR(100) NOT NULL,NURASYMASID BIGINT NOT NULL,PREKEID BIGINT NOT NULL,MATAVIMOVIENETASID BIGINT NOT NULL,KIEKIS NUMERIC(128) NOT NULL,FOREIGN KEY(MATAVIMOVIENETASID) REFERENCES PUBLIC.MATAVIMOVIENETAS(ID),FOREIGN KEY(PREKEID) REFERENCES PUBLIC.PREKES(ID))
CREATE VIEW PUBLIC.VNURASYMAI (ID,NUMERIS,STATUSAS,DATA,IMONE) AS SELECT N.ID,N.NUMERIS,N.STATUSAS,N.DATA,I.PAVADINIMAS AS IMONE FROM PUBLIC.NURASYMAI N LEFT JOIN PUBLIC.IMONES I ON N.IMONEID=I.ID
CREATE MEMORY TABLE PUBLIC.GYVUNURUSYS(ID BIGINT NOT NULL PRIMARY KEY,PAVADINIMAS VARCHAR(100) NOT NULL,UNIQUE(PAVADINIMAS))
CREATE MEMORY TABLE PUBLIC.GYDOMUGYVUNUZURNALAS(ID BIGINT NOT NULL PRIMARY KEY,EILESNUMERIS BIGINT,IMONEID BIGINT NOT NULL,LAIKYTOJAS VARCHAR(200) NOT NULL,REGISTRACIJOSDATA TIMESTAMP NOT NULL,PIRMUPOZYMIUDATA TIMESTAMP,DIAGNOZE VARCHAR(500),GYVUNUSARASAS VARCHAR(500),GYDYMAS VARCHAR(500),ISLAUKAMESAI TIMESTAMP,ISLAUKAPIENUI TIMESTAMP,FOREIGN KEY(IMONEID) REFERENCES PUBLIC.IMONES(ID))
CREATE MEMORY TABLE PUBLIC.ZURNALOGYVUNAI(ID BIGINT NOT NULL PRIMARY KEY,ZURNALOID BIGINT NOT NULL,GYVUNORUSISID BIGINT NOT NULL,NUMERIS VARCHAR(400),AMZIUS VARCHAR(100),FOREIGN KEY(ZURNALOID) REFERENCES PUBLIC.GYDOMUGYVUNUZURNALAS(ID))
CREATE MEMORY TABLE PUBLIC.ZURNALOVAISTAI(ID BIGINT NOT NULL PRIMARY KEY,ZURNALOID BIGINT NOT NULL,PREKEID BIGINT NOT NULL,RECEPTAS VARCHAR(400),MATAVIMOVIENETASID BIGINT NOT NULL,KIEKIS NUMERIC(128) NOT NULL,FOREIGN KEY(ZURNALOID) REFERENCES PUBLIC.GYDOMUGYVUNUZURNALAS(ID),FOREIGN KEY(PREKEID) REFERENCES PUBLIC.PREKES(ID),FOREIGN KEY(MATAVIMOVIENETASID) REFERENCES PUBLIC.MATAVIMOVIENETAS(ID))
CREATE VIEW PUBLIC.VGYVUNAI (ID,NUMERIS,GYVUNORUSISID,LAIKYTOJAS) AS SELECT L.ID,L.NUMERIS,L.GYVUNORUSISID,I.LAIKYTOJAS FROM PUBLIC.ZURNALOGYVUNAI L LEFT JOIN PUBLIC.GYDOMUGYVUNUZURNALAS I ON L.ZURNALOID=I.ID
CREATE MEMORY TABLE PUBLIC.LIKUCIAI(ID BIGINT NOT NULL PRIMARY KEY,DOKUMENTAS VARCHAR(100),IRASOID BIGINT,PREKEID BIGINT NOT NULL,IMONEID BIGINT NOT NULL,MATAVIMOVIENETASID BIGINT NOT NULL,KIEKIS NUMERIC(128) NOT NULL,GALIOJAIKI TIMESTAMP,SERIJA VARCHAR(100),DATA TIMESTAMP NOT NULL,ARSASKAITA BIT(1) NOT NULL,ZURNALOID BIGINT,ZURNALOVAISTOID BIGINT,SASKAITOSID BIGINT,SASKAITOSPREKESID BIGINT,PIRMINISID BIGINT,FOREIGN KEY(MATAVIMOVIENETASID) REFERENCES PUBLIC.MATAVIMOVIENETAS(ID),FOREIGN KEY(PREKEID) REFERENCES PUBLIC.PREKES(ID),FOREIGN KEY(IMONEID) REFERENCES PUBLIC.IMONES(ID),FOREIGN KEY(ZURNALOID) REFERENCES PUBLIC.GYDOMUGYVUNUZURNALAS(ID),FOREIGN KEY(ZURNALOVAISTOID) REFERENCES PUBLIC.ZURNALOVAISTAI(ID),FOREIGN KEY(SASKAITOSID) REFERENCES PUBLIC.SASKAITOS(ID),FOREIGN KEY(SASKAITOSPREKESID) REFERENCES PUBLIC.SASKAITOSPREKES(ID),FOREIGN KEY(PIRMINISID) REFERENCES PUBLIC.LIKUCIAI(ID))
CREATE VIEW PUBLIC.VLIKUCIAIINT (PREKEID,MATAVIMOVIENETASID,IMONEID,KIEKIS,DATA,ID,PAJAMUOTA) AS SELECT L.PREKEID,MIN(L.MATAVIMOVIENETASID)AS MATAVIMOVIENETASID,L.IMONEID,SUM(L.KIEKIS)AS KIEKIS,MAX(L.DATA)AS DATA,MIN(L.ID)AS ID,SUM(CASE WHEN L.KIEKIS>0 THEN L.KIEKIS ELSE 0 END)AS PAJAMUOTA FROM PUBLIC.LIKUCIAI L GROUP BY L.PREKEID,L.IMONEID
CREATE VIEW PUBLIC.VLIKUCIAI (ID,KIEKIS,PAJAMUOTA,DATA,IMONE,MATAVIMOVIENETAS,PREKE) AS SELECT L.ID,L.KIEKIS,L.PAJAMUOTA,L.DATA,I.PAVADINIMAS AS IMONE,MV.KODAS AS MATAVIMOVIENETAS,P.PAVADINIMAS AS PREKE FROM PUBLIC.VLIKUCIAIINT L LEFT JOIN PUBLIC.IMONES I ON L.IMONEID=I.ID LEFT JOIN PUBLIC.PREKES P ON L.PREKEID=P.ID LEFT JOIN PUBLIC.MATAVIMOVIENETAS MV ON MV.ID=L.MATAVIMOVIENETASID
CREATE VIEW PUBLIC.VPREKES (ID,PAVADINIMAS,ISLAUKAMESAI,ISLAUKAPIENUI,MATVIENETAS) AS SELECT P.ID,P.PAVADINIMAS,P.ISLAUKAMESAI,P.ISLAUKAPIENUI,V.KODAS AS MATVIENETAS FROM PUBLIC.PREKES P LEFT JOIN PUBLIC.MATAVIMOVIENETAS V ON P.MATVIENETASID=V.ID
CREATE VIEW PUBLIC.VGYDOMUGYVUNUZURNALAS (ID,EILESNUMERIS,LAIKYTOJAS,REGISTRACIJOSDATA,GYVUNUSARASAS,GYDYMAS,ISLAUKAMESAI,ISLAUKAPIENUI,IMONE) AS SELECT N.ID,N.EILESNUMERIS,N.LAIKYTOJAS,N.REGISTRACIJOSDATA,N.GYVUNUSARASAS,N.GYDYMAS,N.ISLAUKAMESAI,N.ISLAUKAPIENUI,I.PAVADINIMAS AS IMONE FROM PUBLIC.GYDOMUGYVUNUZURNALAS N LEFT JOIN PUBLIC.IMONES I ON N.IMONEID=I.ID
