<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.aireno.vapas.service.persistance">

    <class name="Nustatymas" table="Nustatymai">
        <id name="id" column="Id">
            <generator class="increment"/>
        </id>
        <property name="kodas" type="string" length="100" not-null="true" unique="true"/>
        <property name="reiksme" type="string" length="500" />
    </class>

</hibernate-mapping>
